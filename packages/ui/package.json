    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/vue3-vite": "^10.1.11",
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^20.19.30",
    "@unocss/vite": "^66.5.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/eslint-plugin": "^1.6.6",
