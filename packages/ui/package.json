    "eslint-plugin-oxlint": "~1.39.0",
    "eslint-plugin-vue": "~10.7.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.2",
    "sass-embedded": "^1.97.2",
    "storybook": "^10.2.0",
    "typescript": "~5.9.3",
    "unocss": "^66.5.4",
